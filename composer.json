{
  "name": "aheenam/mozhi",
  "description": "A Laravel Package",
  "license": "MIT",
  "authors": [
  ],
  "minimum-stability": "stable",
  "require": {
    "php" : "^7.2",
    "spatie/yaml-front-matter": "^2.0",
    "Illuminate/Support": "^5.7|^5.8",
    "Illuminate/Routing": "^5.7|^5.8",
    "Illuminate/Http": "^5.7|^5.8",
    "league/commonmark": "^0.16.0",
    "webuni/commonmark-table-extension": "^0.8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0|^8.0",
    "orchestra/testbench": "~3.7.0|~3.8.0",
    "spatie/phpunit-snapshot-assertions": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Aheenam\\Mozhi\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Aheenam\\Mozhi\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Aheenam\\Mozhi\\MozhiServiceProvider"
      ],
      "aliases": {
          "Mozhi": "Aheenam\\Mozhi\\Facades\\Mozhi"
      }
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test:windows": "vendor\\bin\\phpunit"
  }
}
